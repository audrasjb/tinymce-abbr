# Title for the button on the visual editor
#: tinymce-abbr.php:82
msgid "Abbreviation"
msgstr "Abréviation"

# Title of the modal popup
#: tinymce-abbr.php:83
msgid "Insert abbreviation"
msgstr "Insérer une abréviation"

# Title of the button when you are hovering an existing abbreviation
#: tinymce-abbr.php:84
msgid "Delete abbreviation"
msgstr "Supprimer l’abréviation"

# 'title' field in the modal popup
#: tinymce-abbr.php:85
msgid "Title"
msgstr "Titre"

# 'title' help in the modal popup
#: tinymce-abbr.php:86
msgid "The meaning of your abbreviation"
msgstr "Indiquer la signification de l’abréviation"

# 'language' field in the modal popup
#: tinymce-abbr.php:87
msgid "Language (optional)"
msgstr "Langue (facultatif)"

# 'language' field help  in the modal popup
#: tinymce-abbr.php:88
msgid ""
"Example: fr, en, de, etc. Use it only if the abbreviation’s language is "
"different from page main language"
msgstr ""
"Par exemple : fr, en, de, etc. A n’utiliser que si la langue de "
"l'abréviation est différente de la langue globale de la page"

# Javascript alert if you haven't selected any text
#: tinymce-abbr.php:89
msgid "Please select some text first"
msgstr "Veuillez sélectionner du texte d'abord"

